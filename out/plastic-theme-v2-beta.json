  "tokenColors": [
    {
      "name": "#A9B2C3",
      "scope": [
        "meta.brace",
        "punctuation"
      ],
      "settings": {
        "foreground": "#A9B2C3"
      }
    },
    {
      "name": "#61AFEF",
      "scope": [
        "storage",
        "support.class",
        "support.constant"
      ],
      "settings": {
        "foreground": "#61AFEF"
      }
    },
    {
      "name": "#56B6C2",
      "scope": [
        "constant",
        "keyword.other",
        "support.type"
      ],
      "settings": {
        "foreground": "#56B6C2"
      }
    },
    {
      "name": "#E5C07B",
      "scope": [
        "entity.name",
        "variable.language"
      ],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "name": "#D4D7D9",
      "scope": [
        "support.type.property-name",
        "support.variable",
        "variable"
      ],
      "settings": {
        "foreground": "#D4D7D9"
      }
    },
    {
      "name": "#98C379",
      "scope": [
        "string",
        "markup.inline"
      ],
      "settings": {
        "foreground": "#98C379"
      }
    },
    {
      "name": "#5F6672",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "punctuation.terminator.statement"
      ],
      "settings": {
        "foreground": "#5F6672"
      }
    },
    {
      "name": "#E06C75",
      "scope": [
        "keyword",
        "punctuation.definition.keyword"
      ],
      "settings": {
        "foreground": "#E06C75"
      }
    },
    {
      "name": "#D19A66",
      "scope": [
        "entity",
        "entity.name.function",
        "punctuation.definition.entity",
        "support.function"
      ],
      "settings": {
        "foreground": "#D19A66"
      }
    },
    {
      "name": "bold",
      "scope": [
        "markup.bold",
        "punctuation.definition.bold",
        "entity.other.attribute-name.id"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "italic",
      "scope": [
        "comment",
        "markup.italic",
        "punctuation.definition.italic"
