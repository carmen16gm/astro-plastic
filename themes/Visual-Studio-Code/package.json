{
  "name": "vscode",
  "displayName": "Plastic",
  "version": "5.4.0",
  "private": true,
  "description": "A simple theme.",
  "categories": [
    "Themes"
  ],
  "homepage": "https://plastictheme.com/",
  "bugs": {
    "url": "https://github.com/will-stone/Plastic-Theme/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/will-stone/Plastic-Theme"
  },
  "author": "Will Stone",
  "publisher": "will-stone",
  "scripts": {
    "build": "ts-node-script src/index.ts",
    "release": "npm run build && vsce publish"
  },
  "contributes": {
    "themes": [
      {
        "label": "Plastic",
        "uiTheme": "vs-dark",
        "path": "./themes/main.json"
      },
      {
        "label": "Plastic - deprioritised punctuation",
        "uiTheme": "vs-dark",
        "path": "./themes/deprioritised-punctuation.json"
      },
      {
        "label": "Plastic Legacy v2",
        "uiTheme": "vs-dark",
        "path": "./themes/v2.json"
      },
      {
        "label": "Plastic Legacy v2 - deprioritised punctuation",
        "uiTheme": "vs-dark",
        "path": "./themes/v2-deprioritised-punctuation.json"
      }
    ]
  },
  "babel": {
    "plugins": [
      "@babel/plugin-syntax-top-level-await"
    ]
  },
  "dependencies": {
    "tinycolor2": "^1.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/plugin-syntax-top-level-await": "^7.12.13",
    "@types/prettier": "^2.2.3",
    "@types/tinycolor2": "^1.4.2",
    "js-yaml": "^4.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "vsce": "^1.87.1"
  },
  "engines": {
    "node": ">=15.0.0",
    "vscode": "^1.45.0"
  },
  "icon": "images/icon.png",
  "__metadata": {
    "id": "0615a8d1-878b-482f-b36c-773d1ed16adb",
    "publisherId": "744e71fe-bf60-4aba-94b1-d6e77d16fa27",
    "publisherDisplayName": "Will Stone"
  }
}
