{
  "name": "plastic",
  "displayName": "Plastic",
  "description": "A simple theme.",
  "version": "1.11.2",
  "publisher": "will-stone",
  "engines": {
    "vscode": "^1.21.0"
  },
  "categories": ["Themes"],
  "contributes": {
    "themes": [
      {
        "label": "Plastic",
        "uiTheme": "vs-dark",
        "path": "./legacy/plastic-theme.json"
      },
      {
        "label": "Plastic - deprioritised punctuation",
        "uiTheme": "vs-dark",
        "path": "./legacy/plastic-theme-punctuation.json"
      },
      {
        "label": "Plastic v2 Beta",
        "uiTheme": "vs-dark",
        "path": "./out/theme.json"
      },
      {
        "label": "Plastic v2 Beta - deprioritised punctuation",
        "uiTheme": "vs-dark",
        "path": "./out/theme-deprioritised-punctuation.json"
      }
    ]
  },
  "homepage": "https://will-stone.github.io/plastic/",
  "bugs": {
    "url": "https://github.com/will-stone/plastic/issues"
  },
  "icon": "docs/images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/will-stone/plastic"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "esm": "^3.0.38",
    "fs-extra": "^6.0.1",
    "lite-server": "^2.3.0",
    "prettier": "^1.12.1",
    "react": "^16.3.2",
    "tinycolor2": "^1.4.1",
    "vsce": "^1.40.0"
  },
  "scripts": {
    "docs": "lite-server --baseDir='./docs'",
    "vspublish": "vsce publish",
    "build": "node -r esm src/index.js"
  },
  "__metadata": {
    "id": "0615a8d1-878b-482f-b36c-773d1ed16adb",
    "publisherId": "744e71fe-bf60-4aba-94b1-d6e77d16fa27",
    "publisherDisplayName": "Will Stone"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "printWidth": 80,
    "proseWrap": "always",
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "useTabs": false
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "rules": {
      "no-console": 0
    }
  }
}
